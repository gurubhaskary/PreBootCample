const readline = require('readline');
const inp = readline.createInterface({input: process.stdin});
const userInput = [];
inp.on("line", (data) => {userInput.push(data);});inp.on("close", () => 
{
   
    var a= userInput[0];
    var fact = 1;
    for(var i=1;i<=a;i++)
    {
        fact = fact*i;
    }
console.log(fact) ;
    
});
